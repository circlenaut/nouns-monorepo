  "scripts": {
    "dev": "vite",
    "start": "yarn i18n:compile && tsc --project tsconfig.build.json && vite build",
    "debug": "yarn i18n:compile && tsc --project tsconfig.json && vite build --debug",
    "prod": "vite serve",
    "release": "standard-version",
    "serve": "vite preview",
    "test": "vitest",
    "test:ts": "tsc && tsc-alias",
    "test:jest": "jest --transform node_modules/babel-jest",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "yarn tscheck -- --watch",
    "typechain:generate": "yarn typechain --target=ethers-v5 ./src/libs/abi/**/*.json --out-dir=./src/types/typechain",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release",
    "prepare": "yarn i18n:compile",
    "clean": "tsc --build --clean && rimraf dist out yarn-error.log",
    "clean:js": "rimraf **/*.js",
    "clean:modules": "rimraf node_modules",
    "lint": "eslint '{src,functions}/**/*.ts*'",
    "lint:fix": "eslint '{src,functions}/**/*.ts*' --fix",
    "format": "DEBUG=true prettier --check '{src,functions}/**/*.{ts(x)?,sol,md,css,json}'",
    "format:fix": "DEBUG=true prettier --write '{src,functions}/**/*.{ts(x)?,sol,md,css,json}'",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile --typescript",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "postinstall": "yarn i18n"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
